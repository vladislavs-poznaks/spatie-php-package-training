{
    "name": "vladislavs-poznaks/spatie-php-package-training",
    "description": "This is my package spatie-php-package-training",
    "keywords": [
        "vladislavs-poznaks",
        "spatie-php-package-training"
    ],
    "homepage": "https://github.com/vladislavs-poznaks/spatie-php-package-training",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladislavs Poznaks",
            "email": "vladislavs.poznaks@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^1.20",
        "laravel/pint": "^1.2",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "VladislavsPoznaks\\WeightConversion\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VladislavsPoznaks\\WeightConversion\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
